# Terraform-only environment variables
# Copy to .env.terraform (kept out of version control) and fill as needed.

TF_VAR_project_id=
TF_VAR_region=us-central1
TF_VAR_zone=us-central1-a
TF_VAR_name_prefix=json-api
TF_VAR_network_cidr=10.10.0.0/16
TF_VAR_subnet_cidr=10.10.0.0/24
TF_VAR_app_port=3000
TF_VAR_container_image=us-central1-docker.pkg.dev/YOUR_PROJECT/json-api/json-api:latest
TF_VAR_vm_machine_type=e2-small
TF_VAR_vm_disk_size_gb=30
