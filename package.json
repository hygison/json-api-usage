{
  "name": "example",
  "version": "1.0.0",
  "description": "api",
  "main": "dist/main.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "npm run start:prod",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "npm run jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "eslint": "npx eslint --ignore-pattern .gitignore .",
    "eslint:fix": "npx eslint --ignore-pattern .gitignore --fix .",
    "prettier": "npx prettier --check --ignore-pattern .gitignore --ignore-pattern .prettierignore .",
    "prettier:fix": "npx prettier --write --ignore-pattern .gitignore --ignore-pattern .prettierignore .",
    "lint": "npm run eslint ; npm run prettier",
    "format": "npm run eslint:fix && npm run prettier:fix",
    "docker:up": "docker-compose up -d -V --build",
    "migration:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate ./src/database/migrations/migration -d ./src/config/typeorm.config.ts -p",
    "migration:create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli migration:create ./src/database/migrations/migration",
    "migration:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli migration:run -d ./src/config/typeorm.config.ts",
    "migration:revert": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli migration:revert -d ./src/config/typeorm.config.ts",
    "migration:check": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate ./src/database/migrations/migration -d ./src/config/typeorm.config.ts --ch || (echo -e '\u001b[0;31mUnexpected schema changes detected! npm run migration:generate on your local to see the changes and commit new migration.\u001b[0m' && exit 1)",
    "mermaid": "ts-node -r tsconfig-paths/register ./node_modules/erdia/dist/cjs/cli.cjs build -d ./src/config/typeorm.config.ts -o ./ --format md --project-name db"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.929.0",
    "@mikro-orm/core": "^6.1.1",
    "@mikro-orm/knex": "^6.1.1",
    "@mikro-orm/nestjs": "^6.1.1",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^11.1.8",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.8",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/platform-express": "^11.1.8",
    "@nestjs/swagger": "^11.2.1",
    "@nestjs/typeorm": "^11.0.0",
    "@types/nodemailer": "^7.0.3",
    "@types/pg": "^8.15.6",
    "argon2": "^0.44.0",
    "axios": "^1.13.2",
    "body-parser": "^2.2.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "json-api-nestjs": "^9.0.0",
    "lodash": "^4.17.21",
    "nodemailer": "^7.0.10",
    "pg": "^8.16.3",
    "qs": "^6.14.0",
    "randomstring": "^1.3.1",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.1.0",
    "rxjs": "^7.8.2",
    "typeorm": "^0.3.27",
    "typeorm-naming-strategies": "^4.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.8",
    "@types/express": "^5.0.5",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.10.0",
    "@types/randomstring": "^1.3.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "erdia": "^3.7.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-no-relative-import-paths": "^1.6.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-unused-imports": "^4.3.0",
    "jest": "^30.2.0",
    "prettier": "^3.6.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.4"
  }
}
