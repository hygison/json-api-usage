{
  "name": "example",
  "version": "1.0.0",
  "description": "api",
  "main": "dist/main.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "npm run start:prod",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "npm run jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "eslint": "npx eslint --ignore-pattern .gitignore .",
    "eslint:fix": "npx eslint --ignore-pattern .gitignore --fix .",
    "prettier": "npx prettier --check --ignore-pattern .gitignore --ignore-pattern .prettierignore .",
    "prettier:fix": "npx prettier --write --ignore-pattern .gitignore --ignore-pattern .prettierignore .",
    "lint": "npm run eslint ; npm run prettier",
    "format": "npm run eslint:fix && npm run prettier:fix",
    "docker:up": "docker-compose up -d -V --build",
    "migration:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate ./src/database/migrations/migration -d ./src/config/typeorm.config.ts -p",
    "migration:create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli migration:create ./src/database/migrations/migration",
    "migration:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli migration:run -d ./src/config/typeorm.config.ts",
    "migration:revert": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli migration:revert -d ./src/config/typeorm.config.ts",
    "migration:check": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate ./src/database/migrations/migration -d ./src/config/typeorm.config.ts --ch || (echo -e '\u001b[0;31mUnexpected schema changes detected! npm run migration:generate on your local to see the changes and commit new migration.\u001b[0m' && exit 1)",
    "mermaid": "ts-node -r tsconfig-paths/register ./node_modules/erdia/dist/cjs/cli.cjs build -d ./src/config/typeorm.config.ts -o ./ --format md --project-name db"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.665.0",
    "@nestjs/axios": "^3.0.3",
    "@nestjs/common": "^10.4.4",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.4.4",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/platform-express": "^10.4.4",
    "@nestjs/swagger": "^11.0.3",
    "@nestjs/typeorm": "^10.0.2",
    "@types/nodemailer": "^6.4.16",
    "@types/pg": "^8.11.10",
    "argon2": "^0.43.0",
    "axios": "^1.7.7",
    "body-parser": "^1.20.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "json-api-nestjs": "^8.0.0",
    "lodash": "^4.17.21",
    "nodemailer": "^6.9.15",
    "pg": "^8.13.0",
    "randomstring": "^1.3.0",
    "rimraf": "^6.0.1",
    "typeorm": "^0.3.20",
    "typeorm-naming-strategies": "^4.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@nestjs/cli": "^10.4.5",
    "@nestjs/testing": "^10.4.4",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.13",
    "@types/lodash": "^4.17.10",
    "@types/node": "^22.7.4",
    "@types/randomstring": "^1.3.0",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "erdia": "^3.7.1",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-no-relative-import-paths": "^1.5.5",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.8.1"
  }
}
